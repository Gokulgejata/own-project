@model FlagMaterial
<div class="container">
    <div class="row">
        <div class="col-md-12">
            <h2 class="text-uppercase text-center">Delete Details</h2>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12 border-1 border">
            <form method="post" enctype="multipart/form-data">
               
                <input asp-for="@Model.Id" hidden />
                <div class="mb-3">
                    <label asp-for="@Model. MaterialType" class="form-label"></label>
                    <input asp-for="@Model. MaterialType" class="form-control" disabled/>
                   
                </div>
                
               

                <div class="mb-3">
                    <a asp-area="Admin" asp-controller="FlagMaterial" asp-action="Index" class="btn btn-danger">Back</a>
                    <button type="submit" class="btn btn-warning"><i class="bi bi-trash"></i> Delete</button>
                </div>
            </form>
        </div>
        
    </div>
</div>
@section scripts {
    <partial name="_ValidationScriptsPartial" />
}