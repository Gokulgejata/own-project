@model FlagMaterial
<div class="container">
    <div class="card">
       <div class="card-header text-center text-uppercase">
             Flag Material
       </div>
       <div class="card-body">
           <div class="row">
            I
             <div class="col-md-12">
                <div class="row">
                    <div class="col-md-4">
                        <p class="card-text text-uppercase"><strong>Id</strong></p>
                    </div>
                    <div class="col-md-8">
                        <p class="card-text">: @Model.Id</p>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-4">
                        <p class="card-text text-uppercase"><strong>Material type</strong></p>
                    </div>
                    <div class="col-md-8">
                        <p class="card-text">: @Model.MaterialType</p>
                    </div>
                </div>
        
               
           </div>
        <div class="row">
            <div class="col-md-12 text-center">
                <a asp-area="Admin" asp-controller="FlagMaterial" asp-action="Index" class="btn btn-danger">Back</a>
            </div>
        </div>
       </div>
    </div>
</div>
